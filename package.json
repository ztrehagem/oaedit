{
  "name": "@ztrehagem/oaedit",
  "version": "0.1.0",
  "description": "GUI Editor of Open API documents for browsers.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ztrehagem/oaedit.git"
  },
  "keywords": [],
  "author": "Shigeki Suwa <shigeki.suwa@ztrehagem.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ztrehagem/oaedit/issues"
  },
  "homepage": "https://github.com/ztrehagem/oaedit#readme",
  "private": true,
  "packageManager": "pnpm@7.27.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm run lint:eslint && pnpm run lint:prettier",
    "lint:eslint": "eslint --ignore-path=.gitignore --max-warnings=0 .",
    "lint:prettier": "prettier --ignore-path=.gitignore --check .",
    "fix": "pnpm run fix:eslint && pnpm run fix:prettier",
    "fix:eslint": "pnpm run lint:eslint --fix",
    "fix:prettier": "pnpm run lint:prettier --write"
  },
  "dependencies": {
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vanilla-extract/css": "^1.9.5",
    "@vanilla-extract/vite-plugin": "^3.8.0",
    "@vitejs/plugin-react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.4"
  }
